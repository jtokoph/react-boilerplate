{
  "name": "boilerplate",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf build",
    "dev": "NODE_ENV=development node webpack/dev-server.js",
    "test": "NODE_ENV=test jest --coverage",
    "test:watch": "yarn run test -- --watch",
    "build": "yarn run clean && NODE_ENV=production webpack -p --config webpack/config.production.js",
    "lint": "eslint .",
    "lint:fix": "yarn run lint -- --fix"
  },
  "main": "index.js",
  "author": "Jason Tokoph <jason@tokoph.net>",
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "object-assign": "^4.1.1",
    "promise": "^8.0.0",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-redux": "^5.0.5",
    "react-router-dom": "^4.1.1",
    "redux": "^3.7.0",
    "redux-thunk": "^2.2.0",
    "whatwg-fetch": "^2.0.3"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.1",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "^20.0.3",
    "babel-loader": "^7.1.0",
    "babel-preset-env": "^1.5.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "css-loader": "^0.28.4",
    "enzyme": "^2.9.1",
    "eslint": "<4.0.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.6.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "jest": "^20.0.4",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.6",
    "postcss-smart-import": "^0.7.4",
    "precss": "^1.4.0",
    "react-hot-loader": "next",
    "react-test-renderer": "^15.6.1",
    "resolve-url-loader": "^2.1.0",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "webpack-merge": "^4.1.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.jsx",
      "!src/polyfills.js",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "globals": {
      "__DEV__": false,
      "__TEST__": true,
      "__PROD__": false
    }
  }
}
